d1: /home/ml/apps/python/projects/sqlcells/testfiles/sampledatainsnames.xlsx
d2: /home/ml/apps/python/projects/sqlcells/testfiles/sampledatainsurance.xlsx
SQL
select d1.Policy, last_name, first_name, Expiry, State, InsuredValue, email
	from d1, d2
	where d1.Policy = d2.Policy
	order by State, Expiry, last_name
OUTPUT
/home/ml/apps/python/projects/sqlcells/out.xlsx
